@using Models
@{
    List<Models.MapModel.RaidersViewModel> NewestRaiders = (List<Models.MapModel.RaidersViewModel>)ViewData["MostGreatRaiders"];   
}
<aside>
    <a href="/PersonalUser/GiftBox" target="_blank" class="mygiftbox">我的礼包箱</a>
    <div class="kfkckuai">
        <div class="homekf2">
            <h5><span>最新开服</span><a href="/OPenServices/ServiceIndex">更多>></a></h5>
            <p class="kfkckuaiP">
                <span class="intime">时间</span>
                <span class="gamename">游戏名称</span>
                <span class="linesys">运营商</span>
                <span class="caozuo">操作</span>
            </p>
            <ul class="homekc2Ul">
              
            </ul>
        </div>
    </div>
    <div class="welfare">
        <h5><span>最强福利</span><a href="/Home/DemoList">更多>></a></h5>
        <ul>
            @foreach (var item in (List<Models.SqlMapModel.Sql_GameDemo>)ViewBag.NewsetGameDemo)
            {
                <li>
                    <a href="/Home/DemoDetails/@item.Id">
                        <img src="/Content/GameImg/@item.Img" alt="">
                        <p>@item.GameName</p>
                        <p>奖励<b>@item.PaySum</b>爽币</p>
                        <span>立即试玩</span>
                    </a>
                </li>
            }
        </ul>
    </div>
    <div class="extendstrate">
        <h5><span>最赞攻略</span><a href="/Raiders/RaidersIndex">更多>></a></h5>
        @for (int i = 0; i < NewestRaiders.Count; i++)
        {
            if (NewestRaiders[i].EditTitle != null && NewestRaiders[i].EditTitle != "")
            {
                <p>
                    <i class="@Html.Raw('i')@i"></i><a target="_blank" href="/Raiders/RaidersDetail/@NewestRaiders[i].Id"
   title="@NewestRaiders[i].EditTitle">@NewestRaiders[i].EditTitle</a>
                                                </p>
            }
            else
            {
                <p>
                    <i class="@Html.Raw('i')@i"></i><a target="_blank" href="/Raiders/RaidersDetail/@NewestRaiders[i].Id"
   title="@NewestRaiders[i].Title">@NewestRaiders[i].Title</a>
                                                </p>
            }
        }
    </div>
</aside>